2d_physics/layer_2="Player"
2d_physics/layer_3="Projectiles"
2d_physics/layer_4="Breakable"
2d_physics/layer_5="Pickable"

[physics]

common/enable_pause_aware_picking=true

[rendering]

renderer/rendering_method="gl_compatibility"
